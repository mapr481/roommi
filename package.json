        "browser-sync-webpack-plugin": "^2.2.2",
        "case-sensitive-paths-webpack-plugin": "^2.3.0",
        "cross-env": "^7.0",
        "deepmerge": "^4.2.2",
        "fibers": "^5.0.0",
        "jquery": "^3.2",
        "laravel-mix": "^5.0.1",
        "lodash": "^4.17.13",
        "material-design-icons-iconfont": "^5.0.1",
        "popper.js": "^1.12",
        "resolve-url-loader": "^2.3.1",
        "sass": "^1.26.5",
